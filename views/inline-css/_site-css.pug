style(type='text/css').
    /*-----------------------*/
    /* Site Default Template */
    /*-----------------------*/

    :root {
        --orange: #f16624;
        --light-gray: #a3aaad;
        --gray: #7c7d7d;
        --dark-gray: #434647;
        --default-font: sans-serif, arial, "Helvetica Neue", Helvetica, "Lucida Grande";
    }

    h1 {
        font-size: 3em;
        font-family: var(--default-font) !important;
        font-weight: 500 !important;
        text-transform: uppercase;
        margin: 0 0 20px;
        color: var(--dark-gray);
        letter-spacing: -1px;
        line-height: 1em;
    }

    h2 {
        font-size: 1.6em;
        font-family: var(--default-font) !important;
        font-weight: 100 !important;
        text-transform: uppercase;
        margin: 0 0 20px;
        color: var(--orange);
        letter-spacing: -.75px;
        line-height: 1.2;
    }

    h3 {
        font-size: 1.3em;
        font-family: var(--default-font);
        margin: 20px 0;
        color: var(--orange);
        font-weight: 200 !important;
        text-transform: uppercase;
    }

    h4 {
        font-size: 1.1em;
        font-family: var(--default-font);
        font-weight: 100 !important;
        text-transform: uppercase;
        margin: 10px 0;
    }

    p {
        font-weight: 400 !important;
        font-family: var(--default-font);
        margin: 0 0 20px !important;
        letter-spacing: 0.1px;
        font-size: 1.1em;
        line-height: 1.6;
        color: var(--orange);
    }

    hr {
        margin: 40px 0;
        -webkit-box-sizing: content-box;
                box-sizing: content-box;
        height: 1px;
        background: #ccc;
        border: none;
    }

    iframe {
        border: none;
    }

    .embed-container {
        position: relative;
        padding-bottom: 56.25%;
        height: 0;
        overflow: hidden;
        max-width: 100%;
    }
    .embed-container iframe,
    .embed-container object,
    .embed-container embed,
    .embed-container img {
        position: absolute;
        top: 0;
        left: 0;
        width: 100%;
        height: 100%;
    }

    #banner img {
        max-height: 560px;
        -webkit-filter: brightness(100%);
                filter: brightness(100%);
    }

    #banner .banner-page-title h1 {
        background-color: transparent;
        font-size: 3.5em;
        text-shadow: 0 0 100px rgba(0, 0, 0, 0.7);
        text-transform: uppercase;
    }

    @media only screen and (max-width: 992px) {
        #banner .banner-page-title h1 {
            font-size: 3em;
        }
    }

    @media only screen and (max-width: 768px) {
        #banner .banner-page-title h1 {
            font-size: 2em;
        }
    }